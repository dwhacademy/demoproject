DROP TABLE IF EXISTS dev_demo_ml.deployment;

CREATE TABLE dev_demo_ml.deployment (
	start_dttm TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP NOT NULL,
	branch_nm VARCHAR (255) NOT NULL,
	user_nm VARCHAR (255) NOT NULL,
	end_dttm TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (start_dttm)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default
;
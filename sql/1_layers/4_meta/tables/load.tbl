BEGIN TRANSACTION;

CREATE TABLE IF NOT EXISTS dev_demo_ml.load (
	load_id INT,
	load_start_dttm TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP NOT NULL,
	load_end_dttm TIMESTAMP WITH TIME ZONE DEFAULT NULL,
	load_status VARCHAR (255),
	PRIMARY KEY (load_id)
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default
;

CALL dev_demo_ml.sp_deployment_objects('load', 'dev_demo_ml');
END TRANSACTION;
